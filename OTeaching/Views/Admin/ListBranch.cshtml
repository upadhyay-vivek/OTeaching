@model List<OTeaching.Models.CategoryViewModel>

@if(Model.Count()>0)
{
<table class="table">
    <thead class="thead-dark">
        <tr>

            <th>Category Name</th>
            <th>Entry Date</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {

            <tr>
                <td>@category.CName</td>
                <td>@category.EDate</td>
                <td><a href="#">Delete</a></td>
            </tr>

        }
    <tbody>
</table>
}
